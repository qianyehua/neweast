<div class="basic">
    <ul class="form-tab">
        <li class="c">
            <span>操作提示</span>
        </li>
    </ul>
    <div class="formbox">
        <div class="clear"></div>
        <!-- 错误提示 -->
        <div class="nr error-box">
            <div class="nr-t"></div>
            <div class="nr-c" style="height:100px;">
                <div class="text">
                    <h4>#if($!{message})$!{message}#else 操作失败 #end</h4>
					<a href="#if($!{url})$!{appServer}$!{url}#else # #end" class="but" onclick="#if(!$!{url})history.go(-1);return false;#end">返回</a>
                </div>
            </div>
            <div class="nr-b"></div>
        </div>
        <!-- 错误提示 END -->
        <div class="clear"></div>
    </div>
    <div class="clear"></div>
</div>